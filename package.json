{
  "name": "websdk-new",
  "version": "1.0.0",
  "description": "Nerv.js 兼容到IE9",
  "main": "dist/IMLib.js",
  "author": "malei",
  "license": "MIT",
  "engines": {
    "npm": ">=6",
    "node": ">=10"
  },
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js",
    "node": "node ./dist/server.js",
    "pm2": "pm2 start pm2.json",
    "build:qa": "better-npm-run build:qa",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "build:qa": {
      "command": "webpack --progress --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "less": "3.11.1",
    "nervjs": "1.5.6",
    "uuid": "7.0.2",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-transform-react-jsx": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@types/uuid": "7.0.0",
    "babel-loader": "8.0.6",
    "better-npm-run": "0.1.1",
    "css-loader": "2.1.0",
    "html-webpack-plugin": "3.2.0",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "style-loader": "0.23.1",
    "ts-loader": "6.2.1",
    "tslint": "6.0.0-beta0",
    "tslint-react": "4.1.0",
    "typescript": "3.7.4",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  }
}
