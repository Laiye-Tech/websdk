{
  "name": "websdk-new",
  "version": "5.0.0",
  "description": "Nerv.js 兼容到IE10",
  "main": "dist/IMLib.js",
  "author": "malei",
  "license": "MIT",
  "engines": {
    "npm": ">=6",
    "node": ">=10"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "node": "node ./dist/server.js",
    "build:qa": "better-npm-run build:qa",
    "build:pre": "better-npm-run build:pre",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --config webpack.config.js",
      "env": {
        "NODE_ENV": "development",
        "MODE": "dev"
      }
    },
    "build:qa": {
      "command": "webpack --progress --colors",
      "env": {
        "NODE_ENV": "production",
        "MODE": "qa"
      }
    },
    "build:pre": {
      "command": "webpack --progress --colors",
      "env": {
        "NODE_ENV": "production",
        "MODE": "pre"
      }
    },
    "build": {
      "command": "webpack --progress --colors",
      "env": {
        "NODE_ENV": "production",
        "MODE": "prod"
      }
    }
  },
  "dependencies": {
    "core-js": "3.6.5",
    "crypto-js": "4.0.0",
    "less": "3.11.1",
    "nerv-redux": "1.5.6",
    "nervjs": "1.5.6",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "uuid": "7.0.2",
    "web-logger": "1.0.7",
    "whatwg-fetch": "3.0.0",
    "xss": "1.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-transform-react-jsx": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/redux": "3.6.0",
    "@types/redux-actions": "2.6.1",
    "@types/uuid": "7.0.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.0.6",
    "better-npm-run": "0.1.1",
    "css-loader": "2.1.0",
    "html-webpack-plugin": "3.2.0",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "style-loader": "0.23.1",
    "tslint": "6.0.0-beta0",
    "tslint-react": "4.1.0",
    "typescript": "3.7.4",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  }
}
